{{#if thumbnail}}
    <div class="ast-img setbgcolor">
       <span class="ast-type-icon">
           <i class="{{icon}} fw-lg"></i>
       </span>

        <div class="ast-name-img">
            <img alt="thumbnail" class="square-element img-thumbnail" src='{{url ""}}/storage/{{type}}/{{id}}/{{this.thumbnail}}'>
        </div>
    </div>
{{else}}
    {{> asset-default-thumbnail ..}}
{{/if}}
